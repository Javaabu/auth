{
    "name": "javaabu/auth",
    "description": "Laravel Authentication Helpers",
    "homepage": "https://github.com/Javaabu/auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Athfan Khaleel",
            "email": "athfan@javaabu.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "javaabu/helpers": "^1.36",
        "javaabu/activitylog": "^1.1",
        "laravel/ui": "^4.4",
        "javaabu/passport": "^1.1",
        "spatie/laravel-medialibrary": "^10.15 || ^11.0",
        "mobiledetect/mobiledetectlib": "^4.8",
        "javaabu/schema": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Auth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Auth\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
